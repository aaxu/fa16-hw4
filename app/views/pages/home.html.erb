<table>
	<head>
		<title>User Table</title>
	</head>
	<table border = "1">
	<tr>
		<td>Username</td>
		<td>Email</td>
		<td>Age</td>
	</tr>
	<% @users.each do |user| %>
	<tr>
	<td><%=user.username%></td>
	<td><%=user.email%></td>
	<td><%=user.age%></td>
	</tr>
	<% end %>
</table>
<table>
	<head>
		<title>Todo Table</title>
	</head>
	<table border = "1">
	<tr>
		<td>Tasks</td>
		<td>Finished</td>
	</tr>
	<% @todos.each do |todo| %>
	<tr>
	<td><%=todo.tasks%></td>
	<td><%=todo.finished%></td>
	</tr>
	<% end %>
</table>
<table>
	<head>
		<title>Cat Table</title>
	</head>
	<table border = "1">
	<tr>
		<td>Name</td>
	</tr>
	<% @cats.each do |cat| %>
	<tr>
	<td><%=cat.name%></td>
	</tr>
	<% end %>
</table>